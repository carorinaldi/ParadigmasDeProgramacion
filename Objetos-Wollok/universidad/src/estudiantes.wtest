		const unEstudiante = new Estudiante(notas = [ 7, 9 ], materiasEnCurso = [])
		assert.equals(8, unEstudiante.promedio())
	}
	
	test "lucia no desaprobo ningun examen" {
		assert.equals(0, lucia.desaprobo())
	}
	
		test "denise es una estudiante ejemplar" {
		assert.that(denise.esEjemplar())
	}

}

